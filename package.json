    "@testing-library/user-event": "^12.1.10",
    "@types/jest": "^26.0.15",
    "@types/node": "^12.0.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "4.0.3",
    "typescript": "^4.1.2",
    "web-vitals": "^1.0.1"
  },
  "dependencies": {
    "@types/reactstrap": "^8.0.1",
    "@zilliqa-js/zilliqa": "3.3.0",
    "bootstrap": "^4.3.1",
    "husky": "^1.3.1",
    "prettier": "^2.3.2",
    "rc-steps": "^3.3.1",
    "react-app-rewired": "^2.1.8",
    "react-google-recaptcha": "^1.0.5",
    "react-hooks-worker": "^1.0.0",
    "react-icons": "^3.3.0",
    "react-jazzicon": "^0.1.3",
    "react-router-dom": "^5.2.0",
    "reactstrap": "^8.0.0",
    "styled-components": "^4.3.2",
    "ts-jest": "^24.0.2",
    "uuid": "^8.3.2",
    "whatwg-fetch": "^3.0.0",
    "worker-plugin": "^5.0.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn ci"
    }
  },
  "scripts": {
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "test": "react-app-rewired test",
    "eject": "react-scripts eject",
    "test:ci": "CI=true yarn test --coverage --updateSnapshot --forceExit --detectOpenHandles",
    "prettier": "prettier \"**/*.+(ts|tsx|css)\"",
    "format": "npm run prettier -- --write",
    "ci": "npm run prettier -- --list-different && yarn test:ci && tsc --noEmit"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
